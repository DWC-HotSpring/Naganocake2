<h3>配送先編集</h3>
  <table>
    <%= form_with model:@address do |f| %>
      <tr>
        <td>郵便番号</td>
        <td><%= f.text_field :post_code %></td>
      </tr>
      <tr>
        <td>住所（都道府県）</td>
        <td><%= f.text_field :prefecture_name %></td>
        <td>（市区町村）</td>
        <td><%= f.text_field :city %></td>
        <td>（番地）</td>
        <td><%= f.text_field :block %></td>
      </tr>
      <tr>
        <td>氏名</td>
        <td><%= f.text_field :name %></td>
      </tr>
      <tr>
        <td>電話番号</td>
        <td><%= f.text_field :phone_number %></td>
      </tr>
      <tr>
        <td><%= f.submit "編集する", class: "btn btn-primary" %></td>
      </tr>
    <% end %>
  </table>
