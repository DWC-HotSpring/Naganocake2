<%= form_with url: admins_search_path, class:"form-inline my-2 my-lg-0",local: true , method: :get do |f| %>
    <%= f.text_field :content ,class:"form-control mr-sm-2" %>
    <%= f.select :model,
        {"顧客": "customer", "商品": "product"},
        {include_blank: "選択してください"},{ class: 'form-control',required: true} %>
    <%= f.submit '検索', class:"btn btn-outline-success my-2 my-sm-0" %>
<% end %>